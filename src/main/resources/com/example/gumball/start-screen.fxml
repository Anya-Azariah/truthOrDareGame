<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.gumball.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label fx:id="errorMessage"/>
    <Label fx:id="text"/>
    <Button text=" Change!" onAction="#changeScene"/>
    <VBox layoutX="86.0" layoutY="75.0" spacing="4.0">
        <children>
            <Label text="Name"/>
            <TextField fx:id="nametext" prefHeight="25.0" prefWidth="79.0" promptText="Name"/>
            <Label text="Skips"/>
            <TextField fx:id="skipstext" prefHeight="25.0" prefWidth="79.0" promptText="Skips"/>
            <Button text=" SUBMIT" onAction="#addPlayer"/>
        </children>
    </VBox>
</VBox>
